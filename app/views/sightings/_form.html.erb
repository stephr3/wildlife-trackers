<%= render('layouts/errors', :object => @sighting) %>

<%= form_for [@animal, @sighting] do |f| %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :date %>
  <%= f.date_field :date %>
  <%= f.label :neighborhood %>
  <%= f.select(:neighborhood, options_for_select([['North'], ['South'], ['East'], ['West']])) %>
  <%= f.label :latitude %>
  <%= f.text_field :latitude %>
  <%= f.label :longitude %>
  <%= f.text_field :longitude %>
  <p><%= f.submit %></p>
<% end %>
